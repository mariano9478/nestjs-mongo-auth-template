{
  "name": "interactive-forms",
  "version": "1.0.0",
  "description": "ChatBot",
  "author": "Alexandra Lozano Inmigration Law",
  "engines": {
    "node": ">=18.x",
    "npm": ">=9.x"
  },
  "main": "dist/main.js",
  "scripts": {
    "build": "npm run build:clean && nest build --path tsconfig.prod.json",
    "start": "node dist/main.js",
    "dev": "tsnd -r tsconfig-paths/register --inspect=0.0.0.0:9229 --respawn src/main.ts",
    "test": "rimraf coverage .nyc_output && concurrently 'npm:test:unit' 'npm:test:e2e' && npm run calculate-global-test-coverage",
    "test:unit": "vitest run --coverage --config vitest.config.unit.ts",
    "test:e2e": "vitest run --coverage --config ./vitest.config.e2e.ts",
    "test:performance": "k6 run tests/performance/contexts/users/get-users.js",
    "calculate-global-test-coverage": "ts-node scripts/calculate-global-test-coverage.ts",
    "prepare": "husky",
    "lint": "eslint --ignore-path .gitignore . --ext .js,.ts",
    "lint:fix": "npm run lint -- --fix",
    "prettier:fix": "prettier 'src/**/*.ts' --write",
    "build:clean": "rimraf dist; exit 0"
  },
  "dependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/common": "^10.3.8",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.8",
    "@nestjs/mongoose": "^10.0.6",
    "@nestjs/platform-express": "^10.3.9",
    "@nestjs/platform-fastify": "^10.3.8",
    "@nestjs/swagger": "^7.3.1",
    "@types/cookie-session": "^2.0.49",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-session": "^2.1.0",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "envalid": "^7.3.1",
    "helmet": "^7.1.0",
    "joi": "^17.13.1",
    "mongoose": "^8.4.0",
    "nestjs-pino": "^4.1.0",
    "pino-pretty": "^11.2.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/types": "^19.0.3",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.8",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.5.6",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.12.11",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "@vitest/coverage-istanbul": "^1.3.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-unicorn": "^52.0.0",
    "eslint-plugin-vitest": "^0.4.1",
    "fs-extra": "^11.2.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "nock": "^13.5.4",
    "nyc": "^15.1.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.7",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "unplugin-swc": "^1.4.5",
    "vite": "^5.2.11",
    "vitest": "^1.3.1",
    "vitest-mock-extended": "^1.3.1"
  }
}
